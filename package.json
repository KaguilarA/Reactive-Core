{
  "name": "reactive-values",
  "version": "1.0.6",
  "description": "Lightweight reactive value library for TypeScript with support for deep equality and computed values.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "values",
    "utils",
    "types",
    "interface"
  ],
  "scripts": {
    "dev": "vite --config vite.demo.config.ts",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "build-demo": "tsc -p tsconfig.demo.json && vite build --config vite.demo.config.ts",
    "demo": "vite --config vite.demo.config.ts",
    "preview-demo": "vite preview --config vite.demo.config.ts"
  },
  "keywords": [
    "reactive",
    "state-management",
    "typescript",
    "computed",
    "observer",
    "state",
    "reactivity",
    "library"
  ],
  "author": "Kevin Aguilar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KaguilarA/Reactive-Value.git"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "homepage": "https://github.com/KaguilarA/Reactive-Value#readme",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  }
}
